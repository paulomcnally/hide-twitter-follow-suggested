{
  "name": "Hide twitter follow suggested",
  "version": "0.0.1",
  "manifest_version" : 2,
  "description": "Hide twitter follow suggested",
  "permissions": [
    "https://twitter.com/*"
  ],
  "icons": {
    "16" : "icon-16.png",
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css" : ["style.css"]
    }
  ]
}
